<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="bookstore" match-name="book"/>
  <table-filter match-catalog="bookstore" match-name="book_order"/>
  <table-filter match-catalog="bookstore" match-name="category"/>
